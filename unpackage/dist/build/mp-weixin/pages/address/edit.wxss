.u-form-item{color:#000!important}.u-input__input{color:#000!important}.u-column-item{color:#000!important}.u-input__right-icon{padding-right:60rpx}#editaddress{background-color:#f0f0f0}#editaddress .info{padding:0 34rpx;background:#fff}#editaddress .info .button{font-size:28rpx;color:#c0c4cc}#editaddress .pass{background-color:#fff;width:750rpx;height:88rpx;display:flex;align-items:center;justify-content:stretch;margin-top:12rpx}#editaddress .pass .default{margin-left:34rpx;display:flex;align-items:center;justify-content:space-between;flex:1;color:#000}#editaddress .pass .default .checked{margin-right:32rpx}#editaddress .overs{display:flex;align-items:center;justify-content:center;width:400rpx;height:68rpx;position:absolute;left:20%;bottom:27px;background-color:#3e8bf3;color:#fff;font-weight:700;border-radius:100rpx;letter-spacing:2px}#selectSchool, #selectClass{position:absolute;left:0;right:0;top:0;background-color:#fff;overflow:none;z-index:100}#selectSchool .select-box, #selectClass .select-box{position:relative}#selectSchool .select-box .search-box, #selectClass .select-box .search-box{position:fixed;top:0;left:1em;right:1em;z-index:199}#selectSchool .select-box .list-box, #selectClass .select-box .list-box{z-index:180;overflow:auto;margin:84rpx 1em 0 1em;padding:0 6rpx;height:calc(100vh - 84rpx)}#selectSchool .select-box .list-box .row, #selectClass .select-box .list-box .row{display:flex;justify-content:space-between;padding:20rpx;border-bottom:#c0c4cc 1px solid}#selectSchool .select-box .list-box .row .title, #selectClass .select-box .list-box .row .title{font-size:54rpx;height:50rpx;line-height:50rpx}#selectSchool .select-box .list-box .row .title input, #selectClass .select-box .list-box .row .title input{font-size:54rpx}