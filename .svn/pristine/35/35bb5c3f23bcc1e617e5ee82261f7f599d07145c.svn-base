#paid {
  background: url(http://120.78.163.4:8082/my/bj.png) no-repeat;
  background-size: contain;
  background-color: #F0F0F0;
}
#paid .list {
  margin: 0 21rpx;
  padding-top: 22rpx;
}
#paid .list .item {
  padding: 0 32rpx;
  padding-bottom: 20rpx;
  background-color: #FFFFFF;
  border-radius: 28rpx;
  padding-bottom: 22rpx;
  flex-direction: column;
}
#paid .list .item .top {
  display: flex;
  justify-content: space-between;
  flex-wrap: nowrap;
}
#paid .list .item .top > .lfte {
  color: #1A1A1A;
  font-size: 28rpx;
  font-weight: bold;
  padding-top: 32rpx;
  padding-bottom: 30rpx;
}
#paid .list .item .top > .lfte .defalt {
  color: #FFFFFF;
  background-color: #3e8bf3;
  border-radius: 4rpx;
}
#paid .list .item .top > .right {
  color: #1A1A1A;
  font-size: 28rpx;
  font-weight: bold;
  padding-top: 32rpx;
  padding-bottom: 30rpx;
}
#paid .list .item .text > .lfte {
  color: #1A1A1A;
  font-size: 28rpx;
  font-weight: bold;
}
#paid .list .item .lower {
  display: flex;
  justify-content: space-between;
  padding-top: 33rpx;
}
#paid .list .item .lower > .lfte {
