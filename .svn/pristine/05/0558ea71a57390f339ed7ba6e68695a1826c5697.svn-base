<view id="Withdrawal"><view class="item"><view class="top"><view class="lfte">提现方式</view><view data-event-opts="{{[['tap',[['openType',['$event']]]]]}}" class="right" bindtap="__e">{{''+typeName+''}}</view><u-select vue-id="74c71191-1" mode="single-column" list="{{typeList}}" value="{{typeShow}}" data-event-opts="{{[['^confirm',[['typeAffirm']]],['^cancel',[['typeCancel']]],['^input',[['__set_model',['','typeShow','$event',[]]]]]]}}" bind:confirm="__e" bind:cancel="__e" bind:input="__e" bind:__l="__l"></u-select></view><block wx:if="{{isBank}}"><view class="top"><view class="lfte">银行卡</view><view data-event-opts="{{[['tap',[['openBank',['$event']]]]]}}" class="right" bindtap="__e">{{''+bankCardName+''}}</view><u-select vue-id="74c71191-2" mode="single-column" list="{{bankList}}" label-name="bankCardName" value-name="bankCardFront" default-value="{{[0]}}" value="{{show}}" data-event-opts="{{[['^confirm',[['affirm']]],['^cancel',[['cancel']]],['^input',[['__set_model',['','show','$event',[]]]]]]}}" bind:confirm="__e" bind:cancel="__e" bind:input="__e" bind:__l="__l"></u-select></view></block><view class="text"><view class="lfte">提现金额</view><view class="right"></view></view><view class="lower"><view class="lfte"><text>¥</text><view class="right"><input type="number" placeholder="请输入可提现金额" data-event-opts="{{[['change',[['quantityChange',['$event']]]],['input',[['__set_model',['','money','$event',[]]]]]]}}" value="{{money}}" bindchange="__e" bindinput="__e"/></view></view></view><view class="balan">{{'当前余额为'+list.userBalance+"元，"}}<text data-event-opts="{{[['tap',[['allMoney',['$event']]]]]}}" style="color:$u-main-color;" bindtap="__e">全部提现</text></view></view><view class="botomBtn"><view data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="confirm" bindtap="__e">确认提现</view></view></view>